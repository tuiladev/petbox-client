{
  "compilerOptions": {
    "paths": {
      "~/*": ["/src/*"],
      "@assets/*": ["/src/assets/*"],
      "@components/*": ["/src/components/*"],
      "@data/*": ["/src/data/*"],
      "@hooks/*": ["/src/hooks/*"],
      "@layout/*": ["/src/layout/*"],
      "@pages/*": ["/src/pages/*"],
      "@redux/*": ["/src/redux/*"],
      "@services/*": ["/src/services/*"],
      "@utils/*": ["/src/utils/*"]
    }
  }
}
